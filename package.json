{
  "name": "mocf",
  "version": "1.1.2",
  "description": "A command-line http mock API server",
  "keywords": [
    "mock",
    "api",
    "server",
    "cli"
  ],
  "main": "dist/index.js",
  "bin": {
    "mocf": "bin/mocf"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean; npm run build",
    "test": "npm lint"
  },
  "files": [
    "bin",
    "dist/**/*.js"
  ],
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/http-proxy": "^1.17.4",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "eslint": "^7.2.0",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.5"
  },
  "eslintConfig": {
    "extends": "standard-with-typescript",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "env": {
      "node": true,
      "browser": false
    }
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  },
  "dependencies": {
    "chokidar": "^3.4.0",
    "commander": "^5.1.0",
    "connect": "^3.7.0",
    "http-proxy": "^1.18.1",
    "morgan": "^1.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ralph-cheng/mocf.git"
  },
  "author": "ralph_cheng@hotmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ralph-cheng/mocf/issues"
  },
  "homepage": "https://github.com/ralph-cheng/mocf#readme"
}
